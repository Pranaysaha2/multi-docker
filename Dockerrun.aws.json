{
    "AWSEBDockerrunVersion": 2,
    "containerDefinations": [
        {
            "name": "client",
            "image": "shebanhdocker/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "shebangdocker/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name":"worker",
            "image":"shebangdocker/multi-worker",
            "hostname":"worker",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"shebangdocker/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort":80
                }
            ],
            "links":["client","server"]
        }
    ]
}